function [data] = qam_sample(xt,yt,spb,k,thresholds,bit_values)
    data = zeros(1,(length(xt)-2*k*spb)/spb*4);
    bit_count = 0;
    for ind=0:length(data)/4 - 1
        q_bits = adc_2_bit(yt(k*spb + ),thresholds,bit_values);
        i_bits = adc_2_bit(xt(ind),thresholds,bit_values);
        data(bit_count+1:bit_count+2) = q_bits;
        data(bit_count+3:bit_count+4) = i_bits;
    end
        
end